{
  "name": "aws-api-explorer",
  "version": "1.0.0",
  "description": "Web application for exploring and testing AWS APIs",
  "scripts": {
    "scrape-calls": "tsx build-tools/scrape-calls.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/server.js",
    "dev": "NODE_ENV=development nodemon --exec tsx src/server.ts",
    "dev:watch": "NODE_ENV=development nodemon --watch src --ext ts,tsx --exec tsx src/server.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prestart": "npm run build",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "aws",
    "api",
    "explorer",
    "htmx",
    "nodejs",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.840.0",
    "@aws-sdk/client-ec2": "^3.843.0",
    "@aws-sdk/client-iam": "^3.840.0",
    "@aws-sdk/client-lambda": "^3.840.0",
    "@aws-sdk/client-s3": "^3.842.0",
    "@aws-sdk/client-sts": "^3.840.0",
    "jsonc-parser":"^3.3.1",
    "express": "^5.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "nodemon": "^3.1.10",
    "tsx": "4.20.3",
    "typescript": "^5.8.3"
  }
}
